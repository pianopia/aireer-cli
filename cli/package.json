    "nanoid": "^5.1.5",
    "node-cron": "^3.0.3",
    "ora": "^7.0.1"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.10.4",
    "@types/node-cron": "^3.0.11",
    "nodemon": "^3.0.2",
    "typescript": "^5.3.3",
    "webpack": "^5.99.9",
